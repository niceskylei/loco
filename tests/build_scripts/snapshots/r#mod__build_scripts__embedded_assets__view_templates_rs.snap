/// Returns a IndexMap of templates in dependency order (parents before children)
#[must_use]
pub fn get_embedded_templates() -> indexmap::IndexMap<String, &'static str> {
    let mut templates = indexmap::IndexMap::new();
    // Base template with no parent
    templates.insert("index.html".to_string(), include_str!("[TEST_ROOT]/assets/views/index.html"));

    templates
}
